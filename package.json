{
  "name": "inline-parameters-extended",
  "displayName": "Inline Parameters Extended for VSCode",
  "description": "Function parameter annotations displaying inline in VSCode",
  "publisher": "RobertOstermann",
  "icon": "icon.png",
  "version": "0.2.0",
  "license": "MIT",
  "homepage": "https://github.com/RobertOstermann/vscode-inline-parameters",
  "bugs": {
    "url": "https://github.com/RobertOstermann/vscode-inline-parameters/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RobertOstermann/vscode-inline-parameters"
  },
  "main": "./out/src/extension.js",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "inline-parameters.toggle",
        "title": "Inline Parameters: Hide / Show"
      }
    ],
    "configuration": [
      {
        "title": "Inline Parameters",
        "properties": {
          "inline-parameters.php.enabled": {
            "type": "boolean",
            "description": "Enable inline parameters for PHP.",
            "default": true
          },
          "inline-parameters.php.showDollar": {
            "type": "boolean",
            "description": "Show the $ character before PHP parameter names.",
            "default": false
          },
          "inline-parameters.lua.enabled": {
            "type": "boolean",
            "description": "Enable inline parameters for Lua.",
            "default": true
          },
          "inline-parameters.java.enabled": {
            "type": "boolean",
            "description": "Enable inline parameters for Java.",
            "default": true
          },
          "inline-parameters.leadingCharacters": {
            "type": "string",
            "description": "Characters to be shown before each parameter annotation.",
            "default": ""
          },
          "inline-parameters.trailingCharacters": {
            "type": "string",
            "description": "Characters to be shown after each parameter annotation.",
            "default": ":"
          },
          "inline-parameters.parameterCase": {
            "type": "string",
            "description": "Forcibly change the case of the inline parameter name.",
            "enum": [
              "normal",
              "lowercase",
              "uppercase"
            ],
            "default": "normal"
          },
          "inline-parameters.showVariadicNumbers": {
            "type": "boolean",
            "description": "Show the number of times a variadic parameter has been called.",
            "default": true
          },
          "inline-parameters.suppressWhenArgumentMatchesName": {
            "type": "boolean",
            "description": "Suppress parameter name hints on arguments whose text is identical to the parameter name.",
            "default": true
          }
        }
      }
    ],
    "keybindings": [
      {
        "command": "inline-parameters.toggle",
        "key": "ctrl+k a",
        "mac": "cmd+k a"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run -S esbuild-base -- --minify",
    "compile": "rimraf out && tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./node_modules/vscode/bin/test",
    "download-api": "vscode-dts dev",
    "postdownload-api": "vscode-dts main",
    "postinstall": "npm run download-api",
    "esbuild-base": "rimraf out && esbuild ./src/extension.ts --bundle --outfile=out/src/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run -S esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run -S esbuild-base -- --sourcemap --watch"
  },
  "devDependencies": {
    "@types/glob": "^7.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^12.12.44",
    "esbuild": "^0.13.10",
    "eslint": "^8.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.9.5",
    "vscode-test": "^1.2.2"
  },
  "dependencies": {
    "babylon": "^7.0.0-beta.47",
    "install": "^0.13.0",
    "java-ast": "^0.3.0",
    "luaparse": "^0.3.0",
    "npm": "^6.14.10",
    "php-parser": "^3.0.0",
    "recast": "^0.16.0"
  }
}
